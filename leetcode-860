class Solution(object):
    def lemonadeChange(self, bills):
        five = 0
        ten = 0
        for dollar in bills:
            if dollar == 5:
                five += 1
            elif dollar == 10:
                if five > 0:
                    five -= 1
                    ten += 1
                else:
                    return False
            else:  # dollar == 20
                if ten > 0 and five > 0:
                    ten -= 1
                    five -= 1
                elif five >= 3:
                    five -= 3
                else:
                    return False
        return True
