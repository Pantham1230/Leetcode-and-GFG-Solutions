class Solution:
    def canServe(self, bills):
        five = 0
        ten = 0
        for dollar in bills:
            if dollar == 5:
                five += 1
            elif dollar == 10:
                if five == 0:
                    return False
                five -= 1
                ten += 1
            elif dollar == 20:
                if ten > 0 and five > 0:
                    ten -= 1
                    five -= 1
                elif five >= 3:
                    five -= 3
                else:
                    return False
        return True
