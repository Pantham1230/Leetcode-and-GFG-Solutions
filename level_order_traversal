"""
class Node:
    def __init__(self, value):
        self.data = value
        self.left = None
        self.right = None
"""
from collections import deque
class Solution:
    def levelOrder(self, root):
        # code here
        if not root:
            return []

        ans=[]
        q=deque([root])
        while(len(q)>0):
            level=[]
            for i in range(len(q)):
                node=q.popleft()
                if(node.left):
                    q.append(node.left)
                if(node.right):
                    q.append(node.right)
                level.append(node.data)
            ans.append(level)
        return ans
